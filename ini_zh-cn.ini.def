# Configuration file description

[Service]
Endpoint=GoogleTranslate         ;使用端点。有关有效值，请参见[翻译部分]（＃转换器）。
FallbackEndpoint=                ;如果特定翻译的主要端点失败，则自动回退到的端点。

[General]
Language=en                      ;要翻译成的语言
FromLanguage=ja                  ;游戏的原始语言。某些端点支持“auto”，但不建议使用

[Files]
Directory=Translation\{Lang}\Text                                   ;要搜索缓存翻译文件的目录。可以使用占位符：{GameExeName}，{Lang}
OutputFile=Translation\{Lang}\Text\_AutoGeneratedTranslations.txt   ;要插入生成翻译的文件。可以使用占位符：{GameExeName}，{Lang}
SubstitutionFile=Translation\{Lang}\Text\_Substitutions.txt         ;包含要应用于翻译之前的替换的文件。可以使用占位符：{GameExeName}，{Lang}
PreprocessorsFile=Translation\{Lang}\Text\_Preprocessors.txt        ;包含要应用于发送文本之前的预处理的文件。可以使用占位符：{GameExeName}，{Lang}
PostprocessorsFile=Translation\{Lang}\Text\_Postprocessors.txt      ;包含要应用于接收文本之后的后处理的文件。可以使用占位符：{GameExeName}，{Lang}

[TextFrameworks]
EnableUGUI=True                  ;是否启用UGUI翻译
EnableNGUI=True                  ;是否启用NGUI翻译
EnableTextMeshPro=True           ;是否启用TextMeshPro翻译
EnableTextMesh=False             ;是否启用TextMesh翻译
EnableIMGUI=False                ;是否启用IMGUI翻译

[Behaviour]
MaxCharactersPerTranslation=200  ;每个文本的最大字符要翻译。最大2500.
IgnoreWhitespaceInDialogue=True  ;是否忽略对话键中的空格，包括换行符
IgnoreWhitespaceInNGUI=True      ;是否忽略NGUI中的空格，包括换行符
MinDialogueChars=20              ;文本被视为对话的最小长度
ForceSplitTextAfterCharacters=0  ;一旦翻译后的文本超过此字符数，将文本拆分为多行
CopyToClipboard=False            ;是否将钩住的文本复制到剪贴板
MaxClipboardCopyCharacters=450   ;一次最多将多少个字符钩住到剪贴板
ClipboardDebounceTime=1.25       ;钩住的文本到达剪贴板所需的秒数。最小值为0.1
EnableUIResizing=True            ;是否启用插件在翻译时提供对UI组件大小的“最佳尝试”
EnableBatching=True              ;是否启用对支持的端点的翻译批处理
UseStaticTranslations=True       ;是否启用对包含的静态翻译缓存的使用
OverrideFont=                    ;是否启用插件在翻译时提供对UI组件大小的“最佳尝试”。Overrides 更新文本组件时用于文本的字体。 NOTE: 仅适用于UGUI
FallbackFont=                    ;为UGUI添加后备字体，以防止特定字符不受支持。建议使用OverrideFont
OverrideFontTextMeshPro=         ;考虑改用FallbackFontTextMeshPro。Overrides 更新文本组件时用于文本的字体。 NOTE: 仅适用于TextMeshPro
FallbackFontTextMeshPro=         ;为TextMeshPro添加后备字体，以防止特定字符不受支持。建议使用FallbackFontTextMeshPro
ResizeUILineSpacingScale=        ;在UI调整大小期间，默认行间距应缩放的十进制值，例如：0.80。 NOTE: 仅适用于UGUI
ForceUIResizing=True             ;指示UI调整大小行为是否应应用于所有UI组件，而不管它们是否被翻译。
IgnoreTextStartingWith=\u180e;   ;指示插件应忽略以某些字符开头的任何字符串。这是一个用';'分隔的列表。
TextGetterCompatibilityMode=False ;指示是否启用“文本获取器兼容模式”。仅在游戏需要时启用。
GameLogTextPaths=                ;指示游戏用于“日志组件”的游戏对象的特定路径，在这些路径中，游戏不断附加或预先附加文本。需要专业知识进行设置。这是一个用';'分隔的列表。
RomajiPostProcessing=ReplaceMacronWithCircumflex;RemoveApostrophes;ReplaceHtmlEntities ;指示对“翻译”后的罗马字文本进行何种类型的后处理。这在某些游戏中可能很重要，因为所使用的字体不支持各种变音符号。这是一个用';'分隔的列表。可能的值：["RemoveAllDiacritics", "ReplaceMacronWithCircumflex", "RemoveApostrophes", "ReplaceHtmlEntities"]
TranslationPostProcessing=ReplaceMacronWithCircumflex;ReplaceHtmlEntities ;指示对翻译文本（非罗马字）进行何种类型的后处理。可能的值：["RemoveAllDiacritics", "ReplaceMacronWithCircumflex", "RemoveApostrophes", "ReplaceWideCharacters", "ReplaceHtmlEntities"]
RegexPostProcessing=None         ;指示对正则表达式的捕获组执行何种类型的后处理。可能的值：["RemoveAllDiacritics", "ReplaceMacronWithCircumflex", "RemoveApostrophes", "ReplaceWideCharacters", "ReplaceHtmlEntities"]
CacheRegexLookups=False          ;指示是否应将正则表达式查找的结果输出到指定的输出文件
CacheWhitespaceDifferences=False ;指示是否应将空格差异输出到指定的输出文件
CacheRegexPatternResults=False   ;指示是否应将正则表达式拆分翻译的完整结果输出到指定的输出文件
GenerateStaticSubstitutionTranslations=False ;指示插件在使用替换时是否应生成不带变量的翻译
GeneratePartialTranslations=False ;指示插件是否应生成部分翻译，以支持在“滚动”时的文本翻译
EnableTranslationScoping=False   ;指示插件是否应解析'TARC'指令并根据这些指令对翻译进行范围限制
EnableSilentMode=False           ;指示插件是否应不打印与翻译相关的成功消息
BlacklistedIMGUIPlugins=         ;指示如果IMGUI窗口程序集/类/方法名称包含此列表中的任何字符串（不区分大小写），则该UI将不会被翻译。需要MonoMod挂钩。这是一个用';'分隔的列表。
OutputUntranslatableText=False   ;指示如果插件认为某些文本无法翻译，则是否应将其输出到指定的OutputFile
IgnoreVirtualTextSetterCallingRules=False; 指示在尝试设置文本组件的文本时是否应忽略虚拟方法调用。在某些情况下，这可能会帮助设置文本组件的文本。
MaxTextParserRecursion=1         ;指示在解析文本以便在不同部分进行翻译时允许多少级递归。这可以与高级场景中的分割正则表达式一起使用。默认值为1，基本上意味着禁用递归。
HtmlEntityPreprocessing=True     ;指示在发送翻译之前是否应预处理和解码HTML实体。一些翻译器在发送HTML实体时会失败。
HandleRichText=True              ;指示是否应启用对富文本（带有标记的文本）的自动处理
PersistRichTextMode=Final        ;指示解析后的富文本应如何持久化。可以是'Fragment'以逐块存储文本，或'Final'以存储整个翻译字符串（不支持替换！）
EnableTranslationHelper=False    ;指示是否应启用与翻译相关的有用日志消息。当根据重定向资源进行翻译时，这可能会很有用
ForceMonoModHooks=False          ;指示插件必须使用MonoMod挂钩，而不是和谐挂钩
InitializeHarmonyDetourBridge=False ;指示插件应初始化和谐偏移桥，这允许和谐挂钩在不存在System.Reflection.Emit的环境中工作（通常此类设置由插件管理器处理，因此在使用插件管理器时请勿使用）
RedirectedResourceDetectionStrategy=AppendMongolianVowelSeparatorAndRemoveAll ;指示插件是否以及如何尝试识别重定向资源，以防止双重翻译。可以是["None", "AppendMongolianVowelSeparator", "AppendMongolianVowelSeparatorAndRemoveAppended", "AppendMongolianVowelSeparatorAndRemoveAll"]
OutputTooLongText=False          ;指示插件是否应输出超过'MaxCharactersPerTranslation'的文本，而不对其进行翻译

[Texture]
TextureDirectory=Translation\{Lang}\Texture ;指示插件应将纹理转储到的目录，以及从加载图像的目录。可以使用占位符：{GameExeName}，{Lang}
EnableTextureTranslation=False   ;指示插件是否应尝试用TextureDirectory目录中的图像替换游戏中的图像
EnableTextureDumping=False       ;指示插件是否应将插件能够替换的纹理转储到TextureDirectory。有重大性能影响
EnableTextureToggling=False      ;指示插件是否应通过ALT+T热键切换翻译时也影响纹理。并非所有纹理都能正常工作。有重大性能影响
EnableTextureScanOnSceneLoad=False ;指示插件是否应在场景加载时扫描纹理。这使插件能够找到并（可能）替换更多的纹理。这可能会对性能产生影响
EnableSpriteRendererHooking=False ;指示插件是否应尝试挂钩SpriteRenderer。这是一个单独的选项，因为SpriteRenderer不能实际挂钩，并且实现的 workaround 可能会对性能在某些情况下产生影响
LoadUnmodifiedTextures=False     ;指示是否应加载未修改的纹理。修改是根据文件名中的哈希值确定的。仅在调试目的启用此选项，因为它可能会导致奇奇怪怪的情况
TextureHashGenerationStrategy=FromImageName ;指示插件如何通过哈希值识别图片。可以是["FromImageName", "FromImageData", "FromImageNameAndScene"]
DuplicateTextureNames=           ;指示游戏中重复的纹理名称。列表用';'分隔。
DetectDuplicateTextureNames=False;指示插件是否应检测重复的纹理名称。
EnableLegacyTextureLoading=False ;指示插件是否应使用不同的策略加载图像，这可能在游戏引擎旧版时相关
CacheTexturesInMemory=True       ;指示插件是否应将所有加载的纹理保持在内存中以获得最佳性能。禁用以减少内存占用

[ResourceRedirector]
PreferredStoragePath=Translation\{Lang}\RedirectedResources ;指示插件首选存储重定向资源的路径。可以使用占位符：{GameExeName}, {Lang}
EnableTextAssetRedirector=False  ;指示插件是否应重定向TextAssets
LogAllLoadedResources=False      ;指示插件是否应记录到控制台所有加载的资产。有用来确定哪些可以挂钩
EnableDumping=False              ;指示插件是否应转储找到的可翻译资源
CacheMetadataForAllFiles=True    ;当文件位于PreferredStoragePath中的ZIP文件中时，这些文件会被索引到内存中，以避免在加载时执行文件检查IO。启用此选项将对物理文件执行相同操作

[Http]
UserAgent=                       ;覆盖API所需的用户代理
DisableCertificateValidation=False ;指示是否应禁用.NET API的证书验证

[TranslationAggregator]
Width=400                        ;翻译聚合窗口的总宽度。
Height=100                       ;翻译聚合窗口的宽度（每个翻译器）。
EnabledTranslators=              ;翻译聚合窗口中已启用的翻译端点的id列表。列表用';'分隔。

[Google]
ServiceUrl=                      ;可选，用于将google API请求定向到不同的URL。可以用来规避GFWoC

[GoogleLegitimate]
GoogleAPIKey=                    ;OPTIONAL, needed if GoogleTranslateLegitimate is configured

[BingLegitimate]
OcpApimSubscriptionKey=          ;OPTIONAL, needed if BingTranslateLegitimate is configured

[Baidu]
BaiduAppId=                      ;OPTIONAL, needed if BaiduTranslate is configured
BaiduAppSecret=                  ;OPTIONAL, needed if BaiduTranslate is configured

[Yandex]
YandexAPIKey=                    ;OPTIONAL, needed if YandexTranslate is configured

[Watson]
Url=                             ;OPTIONAL, needed if WatsonTranslate is configured
Key=                             ;OPTIONAL, needed if WatsonTranslate is configured

[DeepL]
MinDelay=2                       ;OPTIONAL, used for throttling DeepL
MaxDelay=7                       ;OPTIONAL, used for throttling DeepL

[DeepLLegitimate]
ApiKey=                          ;OPTIONAL, required if DeepLLegitimate is configured
Free=False                       ;OPTIONAL, required if DeepLLegitimate is configured

[Custom]
Url=                             ;Optional, needed if CustomTranslated is configured

[LecPowerTranslator15]
InstallationPath=                ;Optional, needed if LecPowerTranslator15 is configured

[LingoCloud]
LingoCloudToken=                 ;Optional, needed if LingoCloudTranslate is configured

[Debug]
EnableConsole=False              ;启用控制台。如果其他插件（管理器）处理此功能，请勿启用
EnableLog=False                  ;启用额外的日志记录以进行调试

[Migrations]
Enable=True                      ;用于启用此配置文件的自动迁移
Tag=4.15.0                        ;表示此插件最后一次执行时的版本标签。请勿编辑
